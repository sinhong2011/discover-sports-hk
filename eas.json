{
  "cli": {
    "version": ">= 12.0.0",
    "requireCommit": false,
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "NODE_ENV": "staging"
      },
      "ios": {
        "simulator": true,
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "test": {
      "distribution": "internal",
      "channel": "test",
      "env": {
        "NODE_ENV": "staging"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "channel": "production",
      "distribution": "store",
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "app-bundle"
      }
    },
    "production-ios": {
      "extends": "production",
      "ios": {
        "buildConfiguration": "Release"
      }
    },
    "production-android": {
      "extends": "production",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "testflight": {
      "extends": "production",
      "distribution": "store",
      "channel": "testflight",
      "env": {
        "NODE_ENV": "production"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6751938181",
        "appleTeamId": "FX3VR69P5K"
      },
      "android": {
        "serviceAccountKeyPath": "${EXPO_GOOGLE_SERVICE_ACCOUNT_KEY_PATH}",
        "track": "production",
        "changesNotSentForReview": true
      }
    },
    "testflight": {
      "ios": {
        "ascAppId": "6751938181",
        "appleTeamId": "FX3VR69P5K"
      },
      "android": {
        "serviceAccountKeyPath": "${EXPO_GOOGLE_SERVICE_ACCOUNT_KEY_PATH}",
        "track": "internal",
        "changesNotSentForReview": false
      }
    }
  }
}
